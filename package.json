{
  "name": "homoeopathy-case",
  "description": "App to take Homoeopathy cases",
  "author": "Shakeel Ansari",
  "version": "0.1.0",
  "main": "app.js",
  "scripts": {
    "ng": "ng",
    "start": "npm-run-all -p ng:serve electron:serve",
    "ng:serve": "ng serve",
    "electron:serve": "wait-on tcp:4200 && electron . --serve",
    "build": "ng build --base-href ./",
    "start:local": "npm run build && electron .",
    "app:dist": "ng build --base-href ./ && electron-builder build && electron-builder install-app-deps"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "devextreme": "^21.2.4",
    "devextreme-angular": "^21.2.7",
    "ngx-electron": "^2.2.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.6",
    "@angular/cli": "~12.2.6",
    "@angular/compiler-cli": "~12.2.0",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "devextreme-cli": "latest",
    "devextreme-themebuilder": "^21.2.4",
    "electron": "^18.2.0",
    "electron-builder": "^23.0.3",
    "electron-debug": "3.2.0",
    "electron-reloader": "1.2.3",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "npm-run-all": "4.1.5",
    "typescript": "~4.3.5",
    "wait-on": "6.0.1"
  }
}
