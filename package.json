{
  "name": "homoeopathy-case",
  "version": "0.1.0",
  "description": "App to take Homoeopathy cases",
  "homepage": "https://github.com/shakeelansari63/homoeopathy-case",
  "author": {
    "name": "Shakeel Ansari",
    "email": "ShakeelAnsari63@Gmail.com"
  },
  "keywords": [
    "angular",
    "angular 13",
    "electron",
    "electron 18",
    "nodejs",
    "typescript",
    "playwright",
    "eslint",
    "sass",
    "windows",
    "mac",
    "linux"
  ],
  "main": "app.js",
  "build": {
    "appId": "com.freak.homoeopathyapp",
    "mac": {
      "icon": "HomoepathyCase/assets/logo.png",
      "category": "MedicalSoftware",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "win": {
      "icon": "HomoepathyCase/assets/logo.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "tar.gz",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "HomoepathyCase/assets/logo.png",
      "category": "MedicalSoftware"
    }
  },
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "ng build --base-href ./ && electron .",
    "build": "ng build --base-href ./",
    "app:dist": "ng build --base-href ./ && electron-builder build --publish=never"
  },
  "dependencies": {
    "@angular/common": "13.3.1",
    "@angular/compiler": "13.3.1",
    "@angular/core": "13.3.1",
    "@angular/forms": "13.3.1",
    "@angular/language-service": "13.3.1",
    "@angular/platform-browser": "13.3.1",
    "@angular/platform-browser-dynamic": "13.3.1",
    "@angular/router": "13.3.1",
    "devextreme": "^21.2.4",
    "devextreme-angular": "^21.2.7",
    "rxjs": "7.5.5",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "13.1.0",
    "@angular-devkit/build-angular": "13.3.1",
    "@angular-eslint/builder": "13.1.0",
    "@angular-eslint/eslint-plugin": "13.1.0",
    "@angular-eslint/eslint-plugin-template": "13.1.0",
    "@angular-eslint/schematics": "13.1.0",
    "@angular-eslint/template-parser": "13.1.0",
    "@angular/cli": "13.3.1",
    "@angular/compiler-cli": "13.3.1",
    "@ngx-translate/core": "14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "@playwright/test": "1.20.2",
    "@types/jasmine": "3.10.3",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "17.0.23",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "conventional-changelog-cli": "2.2.2",
    "devextreme-cli": "latest",
    "devextreme-themebuilder": "^21.2.4",
    "electron": "18.0.1",
    "electron-builder": "22.14.13",
    "electron-debug": "3.2.0",
    "electron-reloader": "1.2.3",
    "eslint": "8.12.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsdoc": "38.1.6",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "jasmine-core": "3.10.1",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.17",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "7.1.0",
    "karma-jasmine": "4.0.2",
    "karma-jasmine-html-reporter": "1.7.0",
    "node-polyfill-webpack-plugin": "1.1.4",
    "npm-run-all": "4.1.5",
    "playwright": "1.20.2",
    "ts-node": "10.7.0",
    "typescript": "~4.5.5",
    "wait-on": "6.0.1",
    "webdriver-manager": "12.1.8"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "browserslist": [
    "chrome 100"
  ]
}
